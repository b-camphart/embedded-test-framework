{
  "name": "obsidian-todo-plugin-vite",
  "private": true,
  "version": "0.0.0",
  "description": "An example plugin for obsidian that uses the embedded testing framework for running integration tests.",
  "author": "b-camphart",
  "type": "module",
  "scripts": {
    "test:unit": "vitest",
    "test:int": "vite build --mode integration --watch",
    "dev": "vite build --mode development --watch",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "obsidian": "1.5.7-1",
    "vitest": "1.3.1"
  }
}
